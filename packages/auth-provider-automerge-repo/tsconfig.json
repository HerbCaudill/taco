{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "useUnknownInCatchVariables": false
  },
  "include": [
    "src/**/*.ts"
  ]
}
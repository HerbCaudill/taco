{
  "name": "taco-chat",
  "version": "1.0.0",
  "author": {
    "name": "Herb Caudill",
    "email": "herb@devresults.com"
  },
  "description": "Demo app for @localfirst/auth",
  "repository": "http://github.com/local-first-web/auth",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production && tsc && vite build",
    "dev:relay": "cross-env DEBUG='lf*' DEBUG_COLORS=1 node ./scripts/start-relay-server.js",
    "dev:serve": "vite --port 3000",
    "dev": "run-p dev:relay dev:serve",
    "dev:cy": "cross-env BROWSER=none CYPRESS_REMOTE_DEBUGGING_PORT=9222 run-p dev test:cy",
    "test:cy": "cypress open",
    "serve": "vite preview"
  },
  "dependencies": {
    "@ibm/plex": "5",
    "@localfirst/auth": "2",
    "@localfirst/relay": "3",
    "@localfirst/relay-client": "3",
    "@testing-library/cypress": "8",
    "@testing-library/jest-dom": "5",
    "@testing-library/react": "12",
    "@testing-library/user-event": "13",
    "@types/debug": "4",
    "@types/jest": "27",
    "@types/mermaid": "8",
    "@types/react": "17",
    "@types/react-dom": "17",
    "@vitejs/plugin-react-refresh": "1",
    "@windmill/react-ui": "0",
    "async-mutex": "0",
    "autoprefixer": "10",
    "buffer": "6",
    "classnames": "2",
    "clipboard": "2",
    "cross-env": "7",
    "cuid": "2",
    "cypress": "9",
    "debug": "4",
    "events": "3",
    "friendly-words": "1",
    "keycode": "2",
    "mermaid": "8",
    "nodemon": "2",
    "npm-run-all": "4",
    "postcss": "8",
    "react": "17",
    "react-dom": "17",
    "tailwindcss": "2",
    "typescript": "4",
    "vite": "2"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}

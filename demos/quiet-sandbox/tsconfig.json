{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "dist",
    "strict": true,
    "target": "ESNext",
    "module": "NodeNext",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
  },
  "include": ["src"]
}

{
  "name": "taco-js",
  "author": {
    "name": "Herb Caudill",
    "email": "herb@devresults.com"
  },
  "description": "Decentralized authentication and authorization for team collaboration",
  "version": "0.2.0",
  "license": "MIT",
  "module": "dist/taco.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && tscpaths -p tsconfig.json -s ./src -o ./dist --silent",
    "test": "tsdx test",
    "lint": "tsdx lint src",
    "prepare": "yarn build",
    "purge": "scripts/purge.bat"
  },
  "peerDependencies": {},
  "dependencies": {
    "@herbcaudill/crypto": "0",
    "fast-memoize": "2",
    "msgpack-lite": "^0.1.26",
    "ramda": "0",
    "xstate": "4"
  },
  "devDependencies": {
    "@herbcaudill/tscpaths": "0",
    "@types/msgpack-lite": "0",
    "@types/ramda": "0",
    "husky": "4",
    "prettier": "2",
    "tsdx": "0",
    "tslib": "1",
    "typescript": "3",
    "v8-profiler-node8": "6"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "no-unused-vars": "off"
    }
  }
}
